{
  "name": "@brightlayer-ui/angular-auth-workflow",
  "description": "Re-usable workflow components for Authentication and Registration within Eaton applications.",
  "version": "2.4.0-beta.0",
  "scripts": {
    "ng": "ng",
    "watch": "ng build -c demo",
    "build": "ng build",
    "test": "ng test",
    "lint": "eslint \"src/**/**.ts\"",
    "lint:fix": "eslint \"src/**/**.ts\" --fix",
    "prettier": "prettier \"src/**/**.{ts,js,json,css,scss,html}\" --write",
    "prettier:check": "prettier \"src/**/**.{ts,js,json,css,scss,html}\" --check",
    "publish:package": "cd dist && set npm_config_yes=true && npx -p @brightlayer-ui/publish blui-publish",
    "tag:package": "npx -p @brightlayer-ui/tag blui-tag -blui-angular-auth-workflow",
    "install:dependencies": "yarn && cd example && yarn",
    "link:workflow": "bash ./scripts/linkWorkflow.sh",
    "start:example": "yarn install:dependencies && yarn link:workflow && cd example && yarn start",
    "coverage": "yarn test --no-watch --code-coverage"
  },
  "prettier": "@brightlayer-ui/prettier-config",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brightlayer-ui/angular-workflows.git"
  },
  "keywords": [
    "angular",
    "workflow",
    "authentication",
    "auth",
    "brightlayer-ui"
  ],
  "author": "brightlayer-ui <brightlayer-ui@eaton.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/brightlayer-ui/angular-workflows/issues"
  },
  "homepage": "https://github.com/brightlayer-ui/angular-workflows#readme",
  "directories": {
    "doc": "docs"
  },
  "peerDependencies": {
    "@brightlayer-ui/angular-components": ">=4.3.0",
    "@brightlayer-ui/colors": "^3.0.1"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.13",
    "@angular-devkit/build-ng-packagr": "~0.1002.3",
    "@angular/animations": "^11",
    "@angular/cdk": "^11",
    "@angular/cli": "^11",
    "@angular/common": "^11",
    "@angular/compiler": "^11",
    "@angular/compiler-cli": "^11",
    "@angular/core": "^11",
    "@angular/forms": "^11",
    "@angular/material": "^11",
    "@angular/platform-browser": "^11",
    "@angular/platform-browser-dynamic": "^11",
    "@angular/router": "11",
    "@brightlayer-ui/angular-components": "6.0.0",
    "@brightlayer-ui/colors": "^3.0.1",
    "@brightlayer-ui/eslint-config": "^2.0.5",
    "@brightlayer-ui/prettier-config": "^1.0.3",
    "@types/jasmine": "~3.5.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.5.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "codecov": "^3.8.2",
    "codelyzer": "~5.2.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.10.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "ng-packagr": "^11.0.3",
    "prettier": "^1.19.1",
    "rxjs": "^6.5.4",
    "ts-node": "~8.6.2",
    "tsickle": "0.39.1",
